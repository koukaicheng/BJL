/* pages/photoDetail/photoDetail.wxss */

.wrap {
    font-size: 28rpx;
}

.title {
    line-height: 100rpx;
    padding-left: 30rpx;
    color: #52b8f5;
}

.info {
    margin: 0 30rpx;
    /* padding-bottom: 30rpx; */
    color: #666;
}

.image {
    margin: 30rpx;
    margin-top: 0;
    border: 1px #f5f5f5 solid;
    border-radius: 10rpx;
    overflow: hidden;
}

.image image {
    width: 100%;
    vertical-align: top;
    /* height: 100%; */
}
